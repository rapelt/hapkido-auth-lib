{
  "name": "auth-library-project",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test auth-lib",
    "lint": "ng lint",
    "test-all": "npm run test-ci && npm run cy:ci",
    "e2e": "cypress open",
    "test-ci": "ng test auth-lib --watch=false --browsers=ChromeHeadless",
    "build-ci": "ng build auth-lib",
    "watch-lib": "watch 'npm run build-ci' ./projects",
    "cy:start:local": "cypress open",
    "cy:run:ci": " $(npm bin)/cypress run --record --key 7b4f492f-dcf7-41f6-b99a-d7a94b1f6b91 --config baseUrl=http://localhost:8000",
    "cy:ci": "npm run build && run-p --race serve:ci cy:run:ci",
    "serve:ci": "angular-http-server --path dist/auth-library-project -a localhost -p 8000"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
